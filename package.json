{
  "name": "performance-dashboard",
  "version": "0.1.0",
  "private": true,
  "description": "Measures startup latency and binary size of Firebase JS SDK.",
  "license": "Apache-2.0",
  "scripts": {
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "tslint -c tslint.json --fix 'src/**/*.ts'",
    "test": "yarn launch:trial",
    "launch:trial": "ts-node src/launcher.ts --trial",
    "launch": "ts-node src/launcher.ts"
  },
  "devDependencies": {
    "@firebase/logger": "^0.1.1",
    "@types/mysql": "^2.15.5",
    "@types/node": "^11.9.4",
    "@types/yargs": "^12.0.9",
    "cheerio": "^1.0.0-rc.2",
    "datalib": "1.9.1",
    "firebase-tools": "6.0.1",
    "js-string-escape": "1.0.1",
    "mysql": "2.16.0",
    "prettier": "^1.16.4",
    "request": "2.88.0",
    "ts-node": "8.0.2",
    "tslib": "^1.9.3",
    "tslint": "^5.13.1",
    "typescript": "3.3.3",
    "webpagetest": "0.3.9",
    "yargs": "^12.0.2"
  }
}
